# Assert that intrinsic Math.pow works fine
.record Math <external>
.function f32 Math.fpow (f32 a0, f32 a1) <external>

.function u1 main(){
    fmovi v0, 0.78539816  # Pi / 4
    fmovi v1, 0.82718718  # Expected result
    call.short Math.fpow, v0, v0
    fcmpl v1
    jnez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
